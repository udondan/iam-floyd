{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "CloudwatchMetricPublishing",
      "Effect": "Allow",
      "Action": "cloudwatch:PutMetricData",
      "Resource": "*",
      "Condition": {
        "StringEquals": {
          "cloudwatch:namespace": "AWS/SocialMessaging"
        }
      }
    }
  ]
}