{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DRSConversionServerPolicy1",
      "Effect": "Allow",
      "Action": [
        "drs:SendClientMetricsForDrs",
        "drs:SendClientLogsForDrs"
      ],
      "Resource": "*"
    },
    {
      "Sid": "DRSConversionServerPolicy2",
      "Effect": "Allow",
      "Action": [
        "drs:GetChannelCommandsForDrs",
        "drs:SendChannelCommandResultForDrs"
      ],
      "Resource": "*"
    }
  ]
}