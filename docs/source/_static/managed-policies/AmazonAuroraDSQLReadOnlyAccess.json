{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DsqlReadOnlyPermissions",
      "Effect": "Allow",
      "Action": [
        "dsql:GetClusterPolicy",
        "dsql:GetCluster",
        "dsql:GetVpcEndpointServiceName",
        "dsql:ListClusters",
        "dsql:ListTagsForResource"
      ],
      "Resource": "*"
    },
    {
      "Sid": "RelatedServicesPermissions",
      "Effect": "Allow",
      "Action": [
        "cloudwatch:GetMetricData"
      ],
      "Resource": "*"
    }
  ]
}