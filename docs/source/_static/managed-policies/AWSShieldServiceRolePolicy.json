{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AWSShield",
      "Effect": "Allow",
      "Action": [
        "wafv2:GetWebACL",
        "wafv2:UpdateWebACL",
        "wafv2:GetWebACLForResource",
