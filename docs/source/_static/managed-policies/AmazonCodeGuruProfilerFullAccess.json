{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": [
        "codeguru-profiler:*",
        "iam:ListRoles",
        "iam:ListUsers",
        "sns:ListTopics",
        "codeguru:*"
      ],
      "Effect": "Allow",
      "Resource": "*"
    },
    {
      "Action": [
        "iam:CreateServiceLinkedRole"
      ],
      "Effect": "Allow",
      "Resource": "arn:aws:iam::*:role/*AWSServiceRoleForCodeGuruProfiler*",
      "Condition": {
        "StringEquals": {
          "iam:AWSServiceName": "codeguru-profiler.amazonaws.com"
        }
      }
    }
  ]
}