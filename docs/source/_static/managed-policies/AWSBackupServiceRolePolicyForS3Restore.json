{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"s3:CreateBucket",
				"s3:ListBucketVersions",
				"s3:ListBucket",
				"s3:GetBucketVersioning",
				"s3:GetBucketLocation",
				"s3:PutBucketVersioning"
			],
			"Resource": [
				"arn:aws:s3:::*"
			]
		},
		{
			"Effect": "Allow",
			"Action": [
				"s3:GetObject",
				"s3:GetObjectVersion",
				"s3:DeleteObject",
				"s3:PutObjectVersionAcl",
				"s3:GetObjectVersionAcl",
				"s3:GetObjectTagging",
				"s3:PutObjectTagging",
				"s3:GetObjectAcl",
				"s3:PutObjectAcl",
				"s3:ListMultipartUploadParts",
				"s3:PutObject"
			],
			"Resource": [
				"arn:aws:s3:::*/*"
			]
		},
		{
			"Effect": "Allow",
			"Action": [
				"kms:DescribeKey",
				"kms:GenerateDataKey"
			],
			"Resource": "*",
			"Condition": {
				"StringLike": {
					"kms:ViaService": "s3.*.amazonaws.com"
				}
			}
		}
	]
}