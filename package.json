{
  "scripts": {
    "pregenerate": "find lib bin -name \"*.js\" -type f -exec rm -vf {} \\;",
    "generate": "tsc bin/*.ts lib/generator/*.ts && node bin/generate.js",
    "build": "jsii",
    "build:watch": "jsii -w",
    "package": "jsii-pacmak",
    "test": "npx ts-node test/main.ts"
  },
  "name": "iam-floyd",
  "description": "AWS IAM policy statement generator with fluent interface",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "author": {
    "name": "Daniel Schroeder",
    "url": "https://www.udondan.com/",
    "twitter": "udondan"
  },
  "awscdkio": {
    "twitter": "udondan"
  },
  "homepage": "https://github.com/udondan/iam-floyd",
  "repository": {
    "type": "git",
    "url": "https://github.com/udondan/iam-floyd.git"
  },
  "bugs": {
    "url": "https://github.com/udondan/iam-floyd/issues"
  },
  "keywords": [
    "aws",
    "iam",
    "policy"
  ],
  "stability": "experimental",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "excludeTypescript": [
      "bin/*",
      "lib/generator/*",
      "test/*"
    ],
    "targets": {
      "python": {
        "distName": "iam-floyd",
        "module": "iam_floyd"
      },
      "dotnet": {
        "namespace": "IAM.Floyd",
        "packageId": "IAM.Floyd"
      },
      "java": {
        "package": "com.udondan.iamFloyd",
        "maven": {
          "groupId": "com.udondan",
          "artifactId": "iam-floyd"
        }
      }
    }
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.21",
    "@types/glob": "^7.1.3",
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.0.27",
    "@types/request": "^2.48.5",
    "@types/request-promise-native": "^1.0.17",
    "cheerio": "^1.0.0-rc.3",
    "colors": "^1.4.0",
    "glob": "^7.1.6",
    "jsii": "^1.10.0",
    "jsii-pacmak": "^1.10.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "ts-morph": "^7.1.3",
    "ts-node": "^8.10.2",
    "typescript": "~3.9.7"
  }
}
